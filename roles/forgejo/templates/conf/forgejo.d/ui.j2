    {% endif %}
  {% endif %}
  {% if forgejo_ui.svg is defined and
        forgejo_ui.svg | count > 0 %}

[ui.svg]
    {% if forgejo_ui.svg.enable_render | default('') | string | length > 0 %}
ENABLE_RENDER = {{ forgejo_ui.svg.enable_render | bodsch.core.config_bool(true_as='true', false_as='false') }}
    {% endif %}
  {% endif %}
