
[actions]
ENABLED = {{ forgejo_actions.enabled | bodsch.core.config_bool(true_as='true', false_as='false') }}
  {% if forgejo_actions.default_actions_url is defined and
        forgejo_actions.default_actions_url | string | length > 0 %}
DEFAULT_ACTIONS_URL = {{ forgejo_actions.default_actions_url }}
  {% endif %}
  {% if forgejo_actions.artifact_retention_days is defined and
        forgejo_actions.artifact_retention_days | string | length > 0 %}
ARTIFACT_RETENTION_DAYS = {{ forgejo_actions.artifact_retention_days }}
  {% endif %}
